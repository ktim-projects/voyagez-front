# Supabase Configuration
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here

# Clés API pour différents clients
API_KEY_FRONTEND=vci_frontend_your_key_here
API_KEY_ADMIN=vci_admin_your_key_here
API_KEY_STATS=vci_stats_your_key_here

# Configuration Rate Limiting
RATE_LIMIT_REQUESTS=30
RATE_LIMIT_WINDOW=60000

# Mode maintenance - Redirige toutes les pages vers /maintenance
MAINTENANCE_MODE=false
MAINTENANCE_ALLOWED_IPS=

# Mode coming soon - Redirige toutes les pages vers /coming-soon (pour le teasing)
COMING_SOON_MODE=false
COMING_SOON_ALLOWED_IPS=

# Environnement
NODE_ENV=production
